{
  "name": "SunWalletBackend",
  "version": "1.0.0",
  "description": "SunWallet backend micro-service, with Biconomy and Gnosis support",
  "main": "index.js",
  "scripts": {
    "start": "tsc && ncp ./src/constants/abi ./build/constants/abi && ncp ./src/util/validator/hostname ./build/util/validator/hostname && concurrently --kill-others \"tsc -w\" \"npm run server\"",
    "server": "nodemon --watch ./build ./build/app.js --ignore node_modules"
  },
  "keywords": [
    "sun-wallet",
    "meta-transactions",
    "gasless",
    "dapp",
    "pocket",
    "biconomy",
    "gnosis",
    "native"
  ],
  "author": "Ararat Tonoyan",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/express-mung": "^0.4.2",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "0.0.38",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mongoose": "^5.2.6",
    "@types/node": "^10.5.1",
    "@types/randomstring": "^1.1.6",
    "@types/request": "^2.47.1",
    "@types/web3": "^1.2.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.3",
    "typescript": "^3.0.1"
